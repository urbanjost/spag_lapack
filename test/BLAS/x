gfortran -L../.. cblat1.f -lrefblas -o cblat1 -z muldefs
gfortran -L../.. cblat2.f -lrefblas -o cblat2 -z muldefs
gfortran -L../.. cblat3.f -lrefblas -o cblat3 -z muldefs
gfortran -L../.. dblat1.f -lrefblas -o dblat1 -z muldefs
gfortran -L../.. dblat2.f -lrefblas -o dblat2 -z muldefs
gfortran -L../.. dblat3.f -lrefblas -o dblat3 -z muldefs
gfortran -L../.. sblat1.f -lrefblas -o sblat1 -z muldefs
gfortran -L../.. sblat2.f -lrefblas -o sblat2 -z muldefs
gfortran -L../.. sblat3.f -lrefblas -o sblat3 -z muldefs
gfortran -L../.. zblat1.f -lrefblas -o zblat1 -z muldefs
gfortran -L../.. zblat2.f -lrefblas -o zblat2 -z muldefs
gfortran -L../.. zblat3.f -lrefblas -o zblat3 -z muldefs

./cblat1 
./cblat2 < cblat2.in
./cblat3 < cblat3.in
./dblat1 
./dblat2 < dblat2.in
./dblat3 < dblat3.in
./sblat1 
./sblat2 < sblat2.in
./sblat3 < sblat3.in
./zblat1  
./zblat2 < zblat2.in
./zblat3 < zblat3.in
